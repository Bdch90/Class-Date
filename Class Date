<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
</head>
<body>
<script>
    var myDate = function (day,month,year)
    {
        this.day=day;
        this.month=month;
        this.year=year;

        this.getDay = function ()
        {
            return this.day;
        };
        this.getMonth = function ()
        {
            return this.month;
        };
        this.getYear = function ()
        {
            return this.year;
        };
        this.setDay = function ()
        {
            this.day=day;
        };
        this.setMonth = function ()
        {
            this.month=month;
        };
        this.setYear = function ()
        {
            this.year = year;
        };
    };
    var newDate = new myDate(25,9,1990);
    var day = newDate.getDay();
    var month = newDate.getMonth();
    var year = newDate.getYear();
    alert(day + '/' + month + '/' + year);

</script>
</body>
</html>
